<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <PropertyGroup>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Authors>DaneSpiritGOD</Authors>
    <Company>NanJing.com</Company>
    <Product>nng.net</Product>
    <Version>1.0.17</Version>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Suites.Core" Version="1.0.4" />
  </ItemGroup>
  <ItemGroup>
    <None Update="x64\nanomsg.dll">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <Pack>true</Pack>
      <PackagePath>lib\netstandard2.0\x64</PackagePath>
      <PackageCopyToOutput>true</PackageCopyToOutput>
    </None>
    <None Update="x86\nanomsg.dll">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <Pack>true</Pack>
      <PackagePath>lib\netstandard2.0\x86</PackagePath>
      <PackageCopyToOutput>true</PackageCopyToOutput>
    </None>
  </ItemGroup>
</Project>